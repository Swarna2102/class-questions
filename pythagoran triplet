#include <iostream>
using namespace std;
int main(){
    int a,b,c;
    for(a=1;a<100;a++){
        for(b=a+1;b<100;b++){
            c=100-a-b;
            if(c>0 && a*a + b*b == c*c){
                cout<<"Pythagorean triplet: "<<a<<", "<<b<<", "<<c<<endl;
            }
        }
    }
    return 0;
}

